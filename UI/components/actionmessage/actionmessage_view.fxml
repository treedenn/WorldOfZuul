<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane stylesheets="@actionmessage.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.components.actionmessage.ActionMessage">
   <children>
      <HBox alignment="CENTER" spacing="8.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="gestureText" layoutX="40.0" layoutY="10.0" styleClass="gestureText" text="Label" />
            <Label fx:id="keyHint" layoutX="10.0" layoutY="10.0" styleClass="keyHint" text="Label" />
            <Label fx:id="actionText" styleClass="actionText" text="Label" />
         </children>
      </HBox>
   </children>
</AnchorPane>
